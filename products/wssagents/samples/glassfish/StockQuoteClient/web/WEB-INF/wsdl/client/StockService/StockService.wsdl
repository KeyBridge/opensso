<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" xmlns:tns="http://sun.com/stockquote.wsdl" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsd1="http://sun.com/stockquote.xsd" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsap10="http://www.w3.org/2006/05/addressing/wsdl" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" targetNamespace="http://sun.com/stockquote.wsdl">
<wsp:Policy wsu:Id="IStockService_policy">
		<wsp:ExactlyOne>
			<wsp:All>
				<wsap10:UsingAddressing/>
			</wsp:All>
		</wsp:ExactlyOne>
</wsp:Policy>
<types>
        <xsd:schema>
            <xsd:import namespace="http://sun.com/stockquote.xsd" schemaLocation="stock.xsd"/>
        </xsd:schema>
</types>
<message name="QuoteRequest">
        <part name="body" element="xsd1:QuoteRequest"/>
    </message>
<message name="QuoteResponse">
        <part name="body" element="xsd1:QuoteResponse"/>
    </message>
<portType name="StockQuotePortType">
        <operation name="GetStockQuote">
            <input message="tns:QuoteRequest"/>
            <output message="tns:QuoteResponse"/>
        </operation>
     </portType>
<binding name="StockQuoteBinding" type="tns:StockQuotePortType">
    <wsp:PolicyReference URI="#IStockService_policy"/>
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="GetStockQuote">
           <soap:operation soapAction="http://sun.com/GetStockQuote"/>
           <input>
               <soap:body use="literal"/>
           </input>
           <output>
               <soap:body use="literal"/>
           </output>
        </operation>
    </binding>
<wsdl:service xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" name="StockService">
<wsdl:port name="StockQuotePortTypePort" binding="tns:StockQuoteBinding">
<soap:address location="http://localhost:8080/StockService/StockService"/>
</wsdl:port>
</wsdl:service>
</definitions>
