<consoleconfig>
<tabs>
    <tab id="1" viewbean="com.sun.identity.console.realm.RMRealmViewBean"
	url="../realm/RMRealm"
	label="tab.realm.label"
	tooltip="tab.realm.tooltip"
	status="tab.realm.status"
	permissions="sunAMRealmService,sunIdentityRepositoryService,iPlanetAMPolicyService" />

        
    <tab id="4" viewbean="com.sun.identity.console.service.SCConfigViewBean"
	url="../service/SCConfig"
	label="tab.configuration.label"
	tooltip="tab.configuration.tooltip"
	status="tab.configuration.status"
	permissions="iPlanetAMAuth,iPlanetAMPasswordResetService,iPlanetG11NSettings,iPlanetAMAdminConsoleService,iPlanetAMClientDetection,iPlanetAMLoggingService,iPlanetAMNamingService,iPlanetAMPlatformService,iPlanetAMSessionService,iPlanetAMPolicy" 
	accesslevel="globalconfig"/>


    <tab id="5" viewbean="com.sun.identity.console.session.SMProfileViewBean"
	url="../session/SMProfile"
	label="tab.session.label"
	tooltip="tab.session.tooltip"
	status="tab.session.status"
	permissions="iPlanetAMSessionService" 
	accesslevel="globalconfig"/>
</tabs>

                         
<profiletabs>
    <profile id="realms">
	<tab id="1" 
	    viewbean="com.sun.identity.console.realm.RealmPropertiesViewBean"
	    url="../realm/RMRealm"
	    label="realm.showmenu.label.properties"
	    tooltip="tab.sub.realm.tooltip"
	    status="tab.sub.realm.status"
	    permissions="sunAMRealmService" />
	<tab id="2" 
	    viewbean="com.sun.identity.console.authentication.AuthPropertiesViewBean" 
	    label="realm.showmenu.label.authentication" 
	    url="../realm/RMRealm"
	    tooltip="tab.sub.authentication.tooltip"
	    status="tab.sub.authentication.status"
	    permissions="iPlanetAMAuth" />
	<tab id="3" 
	    viewbean="com.sun.identity.console.realm.ServicesViewBean"
	    url="../realm/RMRealm"
	    tooltip="tab.sub.services.tooltip"
	    status="tab.sub.services.status"
	    label="realm.showmenu.label.services"
	    permissions="sunAMRealmService,ANY_SERVICE" />
	<tab id="4"
	    viewbean="com.sun.identity.console.realm.IDRepoViewBean"
	    url="../realm/RMRealm"
	    tooltip="tab.sub.repository.tooltip"
	    status="tab.sub.repository.status"
	    label="realm.showmenu.label.repository"
	    permissions="sunIdentityRepositoryService" />
	<tab id="5"
	    viewbean="com.sun.identity.console.delegation.DelegationViewBean"
	    url="../delegation/Delegation"
	    tooltip="tab.sub.delegation.tooltip"
	    status="tab.sub.delegation.status"
	    label="realm.showmenu.label.delegation"
	    delegateUI="true"
	    permissions="sunAMRealmService" />
    	<tab id="6" 
    	    viewbean="com.sun.identity.console.policy.PolicyViewBean"
    	    url="../policy/Policy"
    	    label="tab.sub.policy.label"
    	    tooltip="tab.sub.policy.tooltip"
    	    status="tab.sub.policy.status"
    	    permissions="iPlanetAMPolicyService" />
    	<tab id="7" 
    	    viewbean="com.sun.identity.console.idm.HomeViewBean"
    	    url="../idm/Home"
    	    label="tab.sub.subjects.label"
    	    tooltip="tab.sub.subjects.tooltip"
    	    status="tab.sub.subjects.status"
    	    permissions="sunIdentityRepositoryService" />
    </profile>
</profiletabs>

<servicesconfig>
    <hidden names="iPlanetAMPolicyService, iPlanetAMEntrySpecificService" />
    <consoleservice realmEnableHideAttrName="iplanet-am-admin-console-um-enabled, iplanet-am-admin-console-online-help, iplanet-am-admin-console-custom-jsp-dir, iplanet-am-admin-console-pc-display, iplanet-am-admin-console-ou-display, iplanet-am-admin-console-gc-display, iplanet-am-admin-console-compliance-user-deletion, iplanet-am-admin-console-group-pclist, iplanet-am-admin-console-group-type,iplanet-am-admin-console-dctree,iplanet-am-admin-console-dctree-attr-list,iplanet-am-admin-console-special-search-filters,iplanet-am-admin-console-compliance-admin-groups,iplanet-am-admin-console-role-display-options,iplanet-am-admin-console-role-default-acis,iplanet-am-admin-console-dynamic-aci-list,iplanet-am-console-service-view-bean,iplanet-am-admin-console-default-pc,iplanet-am-admin-console-default-gc,iplanet-am-admin-console-group-default-pc,iplanet-am-admin-console-user-profile-class,iplanet-am-admin-console-user-role-display,iplanet-am-admin-console-user-groups-display,iplanet-am-admin-console-user-groups-subscribe,iplanet-am-admin-console-user-service-display,iplanet-am-admin-console-default-roles,iplanet-am-admin-console-view-menu,iplanet-am-required-services,iplanet-am-admin-console-user-search-key,iplanet-am-admin-console-user-return-attribute,iplanet-am-user-create-notification-list,iplanet-am-user-delete-notification-list,iplanet-am-user-modify-notification-list,iplanet-am-admin-console-display-options,iplanet-am-admin-console-enduser-profile-class,iplanet-am-console-service-listeners,iplanet-am-admin-console-pre-post-processing-modules,iplanet-am-admin-console-invalid-chars,iplanet-am-admin-console-user-password-validation-class" />
    <identityservice order="user,agent,*role,*group" />
</servicesconfig>
</consoleconfig>

