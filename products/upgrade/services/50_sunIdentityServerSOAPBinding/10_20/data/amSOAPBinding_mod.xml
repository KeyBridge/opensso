<!DOCTYPE Requests
    PUBLIC "-//iPlanet//Sun Java System Access Manager 2005Q4 Admin CLI DTD//EN"
    "jar://com/iplanet/am/admin/cli/amAdmin.dtd"
>

<!--
  apply this file by:

  amadmin -u "xxx" -w "yyy" \-\-data amDisco_add.xml

-->

<Requests>
    <SchemaRequests serviceName="sunIdentityServerSOAPBinding"
	SchemaType="Global">
	<RemoveChoiceValues>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:null:null</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:null:X509</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:null:SAML</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:TLS:null</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:TLS:X509</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:TLS:SAML</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>>urn:liberty:security:2003-08:ClientTLS:null</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:ClientTLS:X509</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2003-08:ClientTLS:SAML</Value>
	    </AttributeValuePair>
	</RemoveChoiceValues>
    </SchemaRequests>

    <SchemaRequests serviceName="sunIdentityServerSOAPBinding"
	SchemaType="Global">
	<AddChoiceValues>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10501" value="urn:liberty:security:2003-08:null:null"/>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10502" value="urn:liberty:security:2003-08:null:X509"/>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10503" value="urn:liberty:security:2003-08:null:SAML"/>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10504" value="urn:liberty:security:2004-04:null:Bearer"/>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10505" value="urn:liberty:security:2003-08:TLS:null"/>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10506" value="urn:liberty:security:2003-08:TLS:X509"/>
	    <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10507" value="urn:liberty:security:2003-08:TLS:SAML"/>
            <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10508" value="urn:liberty:security:2004-04:TLS:Bearer"/>
            <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10509" value="urn:liberty:security:2003-08:ClientTLS:null"/>
            <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10510" value="urn:liberty:security:2003-08:ClientTLS:X509"/>
            <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10511" value="urn:liberty:security:2003-08:ClientTLS:SAML"/>
            <ChoiceValue AttributeName="SupportedAuthenticationMechanisms" I18NKey="a10512" value="urn:liberty:security:2004-04:ClientTLS:Bearer"/>
	</AddChoiceValues>
    </SchemaRequests>
    <SchemaRequests serviceName="sunIdentityServerSOAPBinding"
	SchemaType="Global">
	<AddDefaultValues>
	    <AttributeValuePair>
		<Attribute name="RequestHandlerList"/>
		<Value>key=authnsvc|class=com.sun.identity.liberty.ws.authnsvc.AuthnSvcRequestHandlerImpl</Value>
	    </AttributeValuePair>
	    <AttributeValuePair>
		<Attribute name="SupportedAuthenticationMechanisms"/>
		<Value>urn:liberty:security:2004-04:null:Bearer</Value>
		<Value>urn:liberty:security:2004-04:TLS:Bearer</Value>
		<Value>urn:liberty:security:2004-04:ClientTLS:Bearer</Value>
	    </AttributeValuePair>
	</AddDefaultValues>
    </SchemaRequests>
</Requests>   
