<!DOCTYPE Requests
    PUBLIC "-//iPlanet//Sun Java System Access Manager 2005Q4 Admin CLI DTD//EN"
"jar://com/iplanet/am/admin/cli/amAdmin.dtd"
>

<!--
  apply this file by:

  amadmin -u "xxx" -w "yyy" -t amAuth_mod.xml

-->


<Requests>
    <SchemaRequests serviceName="iPlanetAMAuthService"
	SchemaType="Global"
	i18nKey="">
	<AddDefaultValues>
            <AttributeValuePair>
            <Attribute name="iplanet-am-auth-authenticators"/>
		<Value>com.sun.identity.authentication.modules.datastore.DataStore</Value>
            </AttributeValuePair>
	</AddDefaultValues>
    </SchemaRequests>
    <SchemaRequests serviceName="iPlanetAMAuthService"
	SchemaType="Global"
	i18nKey="">
	<AddAttributeSchema fileName="BASEDIR/PRODUCT_DIR/upgrade/services/50_iPlanetAMAuthService/30_40/data/amAuth_addGlobalAttrs.xml" />
    </SchemaRequests>
    <SchemaRequests serviceName="iPlanetAMAuthService"
        SchemaType="Organization">
        <ModifyType>
	    <AttributeValuePair>
	       	<Attribute name="iplanet-am-auth-post-login-process-class"/>
	       	<Value>list</Value>
	    </AttributeValuePair>
        </ModifyType>

    </SchemaRequests>
    <SchemaRequests serviceName="iPlanetAMAuthService"
	SchemaType="Organization"
	i18nKey="">
	<AddAttributeSchema fileName="BASEDIR/PRODUCT_DIR/upgrade/services/50_iPlanetAMAuthService/30_40/data/amAuth_addAttrs.xml" />
    </SchemaRequests>

    <SchemaRequests serviceName="iPlanetAMAuthService"
        SchemaType="Organization">
        <ModifySyntax>
           <AttributeValuePair>
               <Attribute name="iplanet-am-auth-default-auth-level"/>
               <Value>number_range</Value>
           </AttributeValuePair>
        </ModifySyntax>
        <AddStartRange>
           <AttributeValuePair>
               <Attribute name="iplanet-am-auth-default-auth-level"/>
               <Value>0</Value>
           </AttributeValuePair>
        </AddStartRange>
        <AddEndRange>
           <AttributeValuePair>
               <Attribute name="iplanet-am-auth-default-auth-level"/>
               <Value>2147483647</Value>
           </AttributeValuePair>
        </AddEndRange>
    </SchemaRequests>

</Requests>
