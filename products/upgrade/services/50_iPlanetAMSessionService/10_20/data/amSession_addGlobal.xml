<!DOCTYPE Requests
    PUBLIC "-//iPlanet//Sun Java System Access Manager 2005Q4 Admin CLI DTD//EN"
"jar://com/iplanet/am/admin/cli/amAdmin.dtd"
>
<!--
  this file referenced by:

  amadmin -u "xxx" -w "yyy" -t amSession_add.xml

-->

<Requests>
	 <SubSchema name="Site"
		    inheritance="multiple"
		    i18nKey="site">
	<AttributeSchema name="iplanet-am-session-failover"
                   type="single"
                   syntax="boolean"
                   i18nKey="a107">
                   <DefaultValues>
                         <Value>false</Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-store-username"
                   type="single"
                   syntax="string"
                   i18nKey="a108">
                   <DefaultValues>
                         <Value>amsvrusr</Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-store-password"
                   type="single"
                   syntax="password"
                   i18nKey="a109">
                   <DefaultValues>
                         <Value>password</Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-cluster-serverlist"
                   type="single"
                   syntax="string"
                   i18nKey="a110">
                   <DefaultValues>
                         <Value></Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-store-cpl-max-wait-time"
                   type="single"
                   syntax="number_range"
                   rangeStart="0" rangeEnd="2147483647"
                   i18nKey="a111">
                   <DefaultValues>
                         <Value>60000</Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-JDBC-driver-Impl-classname"
                   type="single"
                   syntax="string"
                   i18nKey="a112">
                   <DefaultValues>
                         <Value>com.iplanet.dpro.session.jdbc.HADBConnectionImpl</Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-jdbc-url"
                   type="single"
                   syntax="string"
                   i18nKey="a113">
                   <DefaultValues>
                        <Value></Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-min-pool-size"
                   type="single"
                   syntax="number_range"
                   rangeStart="1" rangeEnd="100"
                   i18nKey="a114">
                   <DefaultValues>
                         <Value>8</Value>
                   </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-session-max-pool-size"
                   type="single"
                   syntax="number_range"
                   rangeStart="1" rangeEnd="100"
                   i18nKey="a115">
                   <DefaultValues>
                         <Value>32</Value>
                   </DefaultValues>
	</AttributeSchema>
    </SubSchema>
</Requests>
