<!DOCTYPE Requests
    PUBLIC "-//iPlanet//Sun Java System Access Manager 2005Q4 Admin CLI DTD//EN"
    "jar://com/iplanet/am/admin/cli/amAdmin.dtd"
>

<!--
  apply this file by:

  amadmin -u "xxx" -w "yyy" \-\-data amAuthnSvc_mod.xml

-->

<Requests>

    <SchemaRequests serviceName="sunIdentityServerAuthnService"
	SchemaType="Global">
	<AddDefaultValues>
	    <AttributeValuePair>
		<Attribute name="MechanismHandlerList"/>
		<Value>key=SSOToken|class=com.sun.identity.liberty.ws.authnsvc.SSOTokenMechanismHandler</Value>
	    </AttributeValuePair>
	</AddDefaultValues>
    </SchemaRequests>
</Requests>   
