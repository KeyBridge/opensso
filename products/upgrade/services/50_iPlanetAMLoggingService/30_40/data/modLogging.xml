<!--  MODIFY REQUESTS -->

<Requests>
<SchemaRequests serviceName="iPlanetAMLoggingService" SchemaType="global">
<ModifyDefaultValues>
     <AttributeValuePair>
               <Attribute name="iplanet-am-logging-time-buffering-status"/>
