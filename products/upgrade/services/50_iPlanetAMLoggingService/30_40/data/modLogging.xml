     </AttributeValuePair>
     <AttributeValuePair>
               <Attribute name="iplanet-am-logging-buffer-size"/>
               <Value>25</Value>
     </AttributeValuePair>
     <AttributeValuePair>
               <Attribute name="iplanet-am-logging-buffer-time-in-seconds"/>
               <Value>60</Value>
     </AttributeValuePair>

</ModifyDefaultValues>
</SchemaRequests>
</Requests>
