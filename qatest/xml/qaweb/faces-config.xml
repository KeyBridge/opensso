<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="1.2"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <view-handler>
            com.sun.rave.web.ui.appbase.faces.ViewHandlerImpl
        </view-handler>
    </application>
    <managed-bean>
        <managed-bean-name>renderManager</managed-bean-name>
        <managed-bean-class>
            com.icesoft.faces.async.render.RenderManager
        </managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>BuildInfoBean</managed-bean-name>
        <managed-bean-class>com.sun.identity.qatest.qaweb.beans.BuildInfoBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/jsp/home.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/jsp/server.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/jsp/server.jsp</from-view-id>
        <navigation-case>
            <from-outcome>module</from-outcome>
            <to-view-id>/jsp/module.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>summary</from-outcome>
            <to-view-id>/jsp/summary.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/jsp/home.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>TriggerAntBean</managed-bean-name>
        <managed-bean-class>com.sun.identity.qatest.qaweb.beans.TriggerAntBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>renderManager</property-name>
            <value>#{renderManager}</value>
        </managed-property>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/jsp/module.jsp</from-view-id>
        <navigation-case>
            <from-outcome>summary</from-outcome>
            <to-view-id>/jsp/summary.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>server</from-outcome>
            <to-view-id>/jsp/server.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/jsp/summary.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/jsp/run.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>module</from-outcome>
            <to-view-id>/jsp/module.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>server</from-outcome>
            <to-view-id>/jsp/server.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>results</from-outcome>
            <to-view-id>/jsp/results.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>ServerBean</managed-bean-name>
        <managed-bean-class>com.sun.identity.qatest.qaweb.beans.ServerBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ModuleBean</managed-bean-name>
        <managed-bean-class>com.sun.identity.qatest.qaweb.beans.ModuleBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SummaryBean</managed-bean-name>
        <managed-bean-class>com.sun.identity.qatest.qaweb.beans.SummaryBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>